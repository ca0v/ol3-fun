{
  "name": "ol3-fun",
  "version": "5.1.3",
  "description": "OpenLayers Functions",
  "main": "built/index.js",
  "scripts": {
    "clean": "rmdir .\\built /s/q",
    "before-push": "npm run clean && npm run build && npm run test",
    "uglify-for-amd": "pushd built && uglifyjs index.max.js --source-map \"content='index.max.js.map',url='index.js.map'\" --output=index.js --compress --mangle && popd",
    "uglify-for-examples": "pushd built && uglifyjs examples.max.js --source-map \"content='examples.max.js.map',url='examples.js.map'\" --output=examples.js --compress --mangle && popd",
    "build-for-amd": "tsc -p tsconfig.json",
    "build-for-examples": "tsc -p examples/tsconfig.json",
    "build-for-karma": "tsc -p tests/tsconfig.json",
    "build": "npm run build-for-amd && npm run build-for-examples && npm run build-for-karma && npm run uglify-for-amd && npm run uglify-for-examples",
    "start-karma": "karma start built/tests/karma.config.js",
    "karma": "npm run build-for-karma && start npm run start-karma && npm run build-for-karma -- -w",
    "live-for-examples": "live-server --port=5000 --wait=2000 --watch=built/examples.max.js --entry-file=rawgit.html?debug=1",
    "install": "npm run build-for-amd & npm run uglify-for-amd",
    "test": "npm run build-for-karma && npm run start-karma -- --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ca0v/ol3-fun.git"
  },
  "keywords": [
    "OpenLayers"
  ],
  "author": "coreyalix@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ca0v/ol3-fun/issues"
  },
  "homepage": "https://github.com/ca0v/ol3-fun/blob/v5.1.3/README.md",
  "dependencies": {
    "jquery": "^3.3.1",
    "ol": "^5.1.3",
    "requirejs": "^2.3.5"
  },
  "devDependencies": {
    "@types/expect": "^1.20.3",
    "@types/jquery": "^3.3.6",
    "@types/mocha": "^5.2.5",
    "@types/ol": "^4.6.2",
    "buble": "^0.19.3",
    "buble-loader": "^0.5.1",
    "expect": "^23.5.0",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "live-server": "^1.2.0",
    "mocha": "^5.2.0",
    "typescript": "^3.0.1",
    "uglify-js": "^3.4.8",
    "webpack": "^4.17.1"
  }
}
